#include "holberton.h"

/**
 * main - check the code for ALX students
 * 
 * Return: Always 0.
 */

int main(void)
{
int r;
  
r = print_sign(98);
_putchar(',');
_putchar(' ');
_putchar(r + '0');
_putchar('\n');
r = _isalpha('o');
_putchar(r + '0');
r = _isalpha(108);
_putchar(r + '0');
r = _isalpha(';');
_putchar(r + '0');
_putchar('\n');  
return (0);
}


