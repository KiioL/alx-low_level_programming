#include <stdio.h>

/**
 * main - Entry point
 * Return: Always 0(success)
 */
int main(void)
{
char c;
int i;
float f;

printf("Size of a char: %d bytes(s)\n",sizeof(c));
printf("Size of an int: %d bytes(s)\n",sizeof(i));
printf("Size of a long int: %l bytes(s)\n",(long)sizeof(i));
printf("Size of a long long int: %l bytes(s)\n",(long long)sizeof(i));
printf("Size of a float: %.2f bytes(s)\n",sizeof(f));
return (0);
}
